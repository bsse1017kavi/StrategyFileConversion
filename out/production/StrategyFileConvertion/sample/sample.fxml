<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="320.0" prefWidth="425.0">
         <children>
            <CheckBox fx:id="isXML" layoutX="49.0" layoutY="160.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="66.0" text="XML" />
            <CheckBox fx:id="isJSON" layoutX="180.0" layoutY="160.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="66.0" text="JSON" />
            <CheckBox fx:id="isCSV" layoutX="297.0" layoutY="160.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="66.0" text="CSV" />
            <Button fx:id="button" layoutX="173.0" layoutY="238.0" mnemonicParsing="false" onAction="#convert" prefHeight="27.0" prefWidth="72.0" text="Convert" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
